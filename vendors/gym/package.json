    "qrcode": "^1.5.4",
    "viem": "^2.30.5",
    "x402-express": "^0.3.4",
    "x402-fetch": "^0.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.11.19",
    "@vercel/node": "^3.0.0",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.4",
